# Configuração da API
GEMINI_API_KEY=sua_chave_api_aqui
GEMINI_MODEL=gemini-1.5-pro
GEMINI_MAX_RETRIES=3
GEMINI_TIMEOUT=120

# Rate Limiting
GEMINI_REQUESTS_PER_MINUTE=60

# Cache
CACHE_ENABLED=true
CACHE_TTL_SECONDS=3600

# Logging
LOG_LEVEL=INFO
LOG_DIR=./logs
LOG_MAX_SIZE_MB=10
LOG_BACKUP_COUNT=5

# Processamento
MAX_PARALLEL_SECTIONS=3
CONVERGENCE_THRESHOLD=0.95
MAX_ITERATIONS_PER_SECTION=5

# Diretórios
TEMP_DIR=./temp
OUTPUT_DIR=./output
CONFIG_DIR=./config
